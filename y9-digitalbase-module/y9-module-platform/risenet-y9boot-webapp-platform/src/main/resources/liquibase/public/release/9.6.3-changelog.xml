    <changeSet id="1701224818020-7" author="shidaobang">
        <addNotNullConstraint columnDataType="INT" columnName="TYPE" tableName="Y9_COMMON_APP_STORE" validate="true"/>
    </changeSet>
    <changeSet id="1701224818020-8" author="shidaobang">
        <addNotNullConstraint columnDataType="INT" columnName="TYPE" tableName="Y9_COMMON_DATASOURCE" validate="true"/>
    </changeSet>

    <changeSet id="1702520031601-9" author="shidaobang">
        <dropColumn
                columnName="SERIAL"
                tableName="Y9_COMMON_TENANT">
        </dropColumn>
    </changeSet>

</databaseChangeLog>