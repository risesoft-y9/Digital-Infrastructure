<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.9.xsd">

    <changeSet id="1701224818020-1" author="shidaobang">
        <addNotNullConstraint columnDataType="INT" columnName="DISPLAY_TYPE" tableName="Y9_COMMON_OPERATION"
                              validate="true"/>
    </changeSet>
    <changeSet id="1701224818020-3" author="shidaobang">
        <addNotNullConstraint columnDataType="INT" columnName="OPEN_TYPE" tableName="Y9_COMMON_APP_STORE"
