	<modelVersion>4.0.0</modelVersion>
	<groupId>net.risesoft</groupId>
	<artifactId>risenet-y9boot-webapp-sso-server-jdk21</artifactId>
	<packaging>war</packaging>
	<version>v9.7.0-SNAPSHOT</version>

	<parent>
		<groupId>net.risesoft</groupId>
